{
  "name": "@andrglo/react-window-grid",
  "version": "0.1.0",
  "description": "A react grid with synced column and row headers",
  "repository": "https://github.com/andrglo/react-window-grid.git",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "scripts": {
    "start": "rollup -c -w & webpack-dev-server",
    "build": "rollup -c --configEnv production",
    "build:test": "rollup -c --configEnv test",
    "build:dev": "rollup -c",
    "test": "jest --bail --coverage --no-cache --testPathIgnorePatterns .history",
    "pretest": "yarn build:test",
    "test:report:coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "react",
    "react-window",
    "virtual",
    "window",
    "windowed",
    "table",
    "grid",
    "spreadsheet"
  ],
  "author": "Andre Gloria <andredagloria@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "react-window": "^1.8.1",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^4.0.4",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "dom-helpers": "^3.4.0"
  }
}
